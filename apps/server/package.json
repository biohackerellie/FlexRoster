{
  "name": "@local/server",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "bun --env-file=../../.env.local run --watch src/index.ts",
    "clean": "rm -rf .turbo node_modules dist",
    "clean:outs": "rm -rf dist",
    "start": "bun run dist/index.js",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "build": "tsc",
    "build:cli": "tsup",
    "build:local": "bun  build  ./src/index.ts --outdir ./dist --target bun",
    "build:bun": "/root/.bun/bin/bun build ./src/index.ts --outdir ./dist --target bun",
    "nightly": "bun  run ./src/scripts/nightly.ts",
    "cache:reset": "bun  run ./src/scripts/nightly/redisInit.ts",
    "with-env": "bun --env-file=../../.env --"
  },
  "dependencies": {
    "@clack/core": "^0.3.4",
    "@clack/prompts": "^0.7.0",
    "@elysiajs/cors": "^1.1.1",
    "@elysiajs/swagger": "^1.1.3",
    "@local/db": "workspace:*",
    "@sinclair/typebox": "^0.33.12",
    "@t3-oss/env-core": "^0.11.1",
    "elysia": "^1.1.17",
    "execa": "^9.4.0",
    "ioredis": "^5.4.1",
    "nanoid": "^5.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@local/eslint-config": "workspace:*",
    "@local/prettier-config": "workspace:*",
    "@local/tsconfig": "workspace:*",
    "@local/utils": "workspace:*",
    "@types/chalk-animation": "^1.6.3",
    "@types/fs-extra": "^11.0.4",
    "@types/gradient-string": "^1.1.6",
    "@types/node": "^20.16.10",
    "bun-types": "^1.1.29",
    "dotenv-cli": "^7.4.2",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "type-fest": "^4.26.1",
    "typescript": "catalog:"
  },
  "prettier": "@local/prettier-config"
}
