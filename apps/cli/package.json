{
  "name": "flexroster-cli",
  "version": "0.0.1",
  "description": "A command line interface for setting up FlexRoster",
  "type": "module",
  "exports": "./dist/index.js",
  "bin": {
    "flexroster-cli": "./dist/index.js"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "typecheck": "tsc",
    "builds": "tsup",
    "devv": "tsup --watch",
    "clean": "rm -rf dist .turbo node_modules",
    "start": "node dist/index.js",
    "lint": "eslint",
    "format": "prettier --check . --ignore-path ../../.gitignore"
  },
  "dependencies": {
    "@clack/core": "^0.3.2",
    "@clack/prompts": "^0.6.3",
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "chalk": "5.2.0",
    "commander": "^10.0.0",
    "execa": "^7.1.1",
    "fs-extra": "^11.2.0",
    "gradient-string": "^2.0.2",
    "ora": "6.3.1",
    "sort-package-json": "^2.4.1"
  },
  "devDependencies": {
    "@local/eslint-config": "workspace:*",
    "@local/prettier-config": "workspace:*",
    "@local/tsconfig": "workspace:*",
    "@local/tailwind-config": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "@types/gradient-string": "^1.1.6",
    "@types/node": "^20.12.7",
    "tsup": "^8.0.2",
    "type-fest": "^4.15.0",
    "typescript": "^5.4.5"
  }
}
